{
    "builds": [{
        "src": "webcam_capture/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.10" }
    }],
    "routes": [
        {
            "src": "/capture/static/(.*)",
            "dest": "webcam_capture/wsgi.py"
        }
    ]
}